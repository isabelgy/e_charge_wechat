<view class="page-body">
  <view class="page-section page-section-gap">
    <map
      id="myMap"
      style="width: 100%; height: 610px;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}" 
      covers="{{covers}}"
      bindmarkertap="click"
      show-location
    >
      <!-- <cover-view class="container" > -->
      <cover-image class="img" src="/image/location.png" bindtap="moveToLocation"/>
    <!-- </cover-view>  -->
    </map>
     
  </view>
